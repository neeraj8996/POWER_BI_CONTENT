        function updateLoadTimestamp(messageData) {
            bootstrapIsUsed = messageData.headers && messageData.headers['bootstrapped'];
            if (bootstrapIsUsed && bootstrapIsDone) {
                viewLoadingStarted = Date.now();
            }
        }



